{
    "name": "botble/slug",
    "description": "Slug/Permalink",
    "type": "package",
    "autoload": {
        "psr-4": {
            "Botble\\Slug\\": "src"
        }
    },
    "require": {
        "botble/platform": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Botble\\Slug\\Providers\\SlugServiceProvider"
            ],
            "aliases": {
                "SlugHelper": "Botble\\Slug\\Facades\\SlugHelperFacade"
            }
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../../platform/core"
        }
    ]
}
