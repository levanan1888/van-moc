{
    "name": "botble/plugin-generator",
    "description": "Plugin generator",
    "type": "package",
    "autoload": {
        "psr-4": {
            "Botble\\PluginGenerator\\": "src"
        }
    },
    "require": {
        "botble/platform": "*",
        "botble/plugin-management": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Botble\\PluginGenerator\\Providers\\PluginGeneratorServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../../platform/core"
        },
        {
            "type": "path",
            "url": "../../../platform/packages/plugin-management"
        }
    ]
}
